<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>
</div>

<div class="form-group">
  <%= f.label :tags %>
  <%= f.text_field :tags, class: 'form-control' %>
</div>

<div class="form-group">
  <div class="form-check">
    <%= f.check_box :shared, class: 'form-check-input' %>
    <%= f.label :shared, class: 'form-check-label' %>
  </div>
</div>

<hr />
