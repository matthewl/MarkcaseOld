<%= form_for @bookmark do |f| %>
  <div class="form-group">
    <%= f.file_field :import_file, class: 'custom-file-input' %>
    <%= f.label :import_file, 'Select your import file ...', class: 'custom-file-label' %>
    <small class="form-text text-muted">Only HTML import files are accepted at this time.</small>
  </div>
<% end %>